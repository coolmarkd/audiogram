{
  "name": "audiogram",
  "version": "0.9.5",
  "description": "Turn audio into a shareable video.",
  "main": "index.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "npm run postinstall && bin/server",
    "worker": "bin/worker",
    "postinstall": "mkdir -p editor/js && browserify client/index.js > editor/js/bundle.js",
    "rebuild": "npm run postinstall",
    "watch": "mkdir -p editor/js && watchify client/index.js -o editor/js/bundle.js",
    "debug": "npm run postinstall && DEBUG=1 bin/server",
    "test": "rm -rf test/tmp && tape 'test/**/*-test.js'",
    "audit:suppress": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix --force",
    "install:safe": "npm install --audit-level=moderate --fund=false",
    "update:deps": "node scripts/update-deps.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nypublicradio/audiogram.git"
  },
  "keywords": [],
  "author": {
    "name": "Noah Veltman",
    "url": "https://twitter.com/veltman"
  },
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1691.0",
    "browserify": "^17.0.0",
    "canvas": "^2.11.2",
    "compression": "^1.7.4",
    "d3": "^7.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jquery": "^3.7.1",
    "mkdirp": "^3.0.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-ffprobe": "^1.2.2",
    "redis": "^4.6.10",
    "rimraf": "^5.0.5",
    "smartquotes": "^2.0.1",
    "underscore": "^1.13.6",
    "uuid": "^9.0.1",
    "webaudio-peaks": "^0.0.5",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "supertest": "^6.3.3",
    "tape": "^5.6.6",
    "watchify": "^4.0.0"
  },
  "overrides": {
    "minimist": "^1.2.8",
    "semver": "^7.5.4"
  },
  "npmConfig": {
    "audit-level": "moderate",
    "fund": false,
    "update-notifier": false
  }
}
